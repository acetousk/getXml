<!--Transition Screen Location component://tools/screen/Tools/ArtifactStats.xml Transition Name actions--><transition name="actions"><actions><!--always-actions at component://webroot/screen/webroot.xml--><set field="html_scripts" from="new LinkedHashSet()"/><set field="footer_scripts" from="new LinkedHashSet()"/><set field="html_stylesheets" from="new LinkedHashSet()"/><!--actions--><set field="levels" from="levels ? levels as int : 2"/><set field="entityInfoList" from="ec.entity.getAllEntityInfo(levels, false)"/><set field="entityCount" from="0"/><set field="viewEntityCount" from="0"/><iterate list="entityInfoList" entry="entityInfo"><set field="entityCount" from="entityCount + entityInfo.entities"/><set field="viewEntityCount" from="viewEntityCount + entityInfo.viewEntities"/></iterate><set field="serviceInfoList" from="ec.service.getAllServiceInfo(levels)"/><set field="allRootScreenLocations" from="ec.screen.getAllRootScreenLocations()"/></actions></transition>