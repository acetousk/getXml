
<!-- Entity Name: mantle.shipment.carrier.ShippingGatewayBoxType sql table: SHIPPING_GATEWAY_BOX_TYPE --><entity entity-name="ShippingGatewayBoxType" package="mantle.shipment.carrier" use="configuration" cache="true">
