<!--Service Location mantle.work.MeasurementServices.get#MeasurementUomsByMeasurementType--><service verb="get" noun="MeasurementUomsByMeasurementType"><in-parameters><parameter name="measurementTypeId" required="true"/></in-parameters><out-parameters><parameter name="resultList" type="List"/></out-parameters><actions><entity-find-one entity-name="mantle.work.measurement.MeasurementType" value-field="measurementType"/><entity-find entity-name="moqui.basic.Uom" list="uomList"><econdition field-name="uomTypeEnumId" from="measurementType?.uomTypeEnumId" ignore-if-empty="true"/><order-by field-name="description"/></entity-find><script><![CDATA[resultList = []
