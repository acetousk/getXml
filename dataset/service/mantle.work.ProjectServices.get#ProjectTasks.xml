<!--Service Location mantle.work.ProjectServices.get#ProjectTasks--><service verb="get" noun="ProjectTasks"><in-parameters><parameter name="rootWorkEffortId" default="workEffortId"/><parameter name="workEffortId"/></in-parameters><out-parameters><parameter name="resultList" type="List"><parameter name="result" type="Map"/></parameter></out-parameters><actions><entity-find entity-name="mantle.work.effort.WorkEffort" list="taskList"><econdition field-name="rootWorkEffortId"/><econdition field-name="workEffortTypeEnumId" value="WetTask"/><econdition field-name="statusId" operator="not-in" value="WeClosed,WeCancelled"/><order-by field-name="workEffortId"/></entity-find><script><![CDATA[resultList = []
