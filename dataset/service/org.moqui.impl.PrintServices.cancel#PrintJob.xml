<!--Service Location org.moqui.impl.PrintServices.cancel#PrintJob--><service verb="cancel" noun="PrintJob"><in-parameters><parameter name="printJobId" entity-name="moqui.basic.print.PrintJob" field-name="printJobId"/></in-parameters><out-parameters/><actions><service-call name="org.moqui.impl.PrintServices.make#CupsClient" in-map="[printJobId:printJobId]" out-map="[cupsClient:context.cupsClient,printJob:context.printJob,networkPrinter:context.networkPrinter]"/><if condition="!cupsClient.cancelJob((int) printJob.jobId)"><then><message><![CDATA[Could not cancel job [${printJob.jobId}] on print server at [${networkPrinter.serverHost}] for PrintJob [${printJobId}]]]></message></then><else><service-call name="update#moqui.basic.print.PrintJob" in-map="[printJobId:printJobId, statusId:'PtjCanceled']"/></else></if></actions></service>