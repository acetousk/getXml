<!--Service Location mantle.account.ReconciliationServices.noReconcile#PaymentMethodTrans--><service verb="noReconcile" noun="PaymentMethodTrans"><in-parameters><parameter name="paymentMethodId" required="true" entity-name="mantle.account.method.PaymentMethodTrans" field-name="paymentMethodId"/><parameter name="fitId" required="true" entity-name="mantle.account.method.PaymentMethodTrans" field-name="fitId"/></in-parameters><out-parameters/><actions><entity-find-one entity-name="mantle.account.method.PaymentMethodTrans" value-field="bankAccountTrans"/><if condition="bankAccountTrans.paymentId || bankAccountTrans.acctgTransId"><return type="warning" message="Bank account ${paymentMethodId} transaction ${fitId} is already reconciled, not setting no reconcile"/></if><set field="bankAccountTrans.noReconcile" value="Y"/><entity-update value-field="bankAccountTrans"/></actions></service>