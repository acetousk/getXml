<!--Service Location mantle.party.PartyServices.get#PartyContentLocation--><service verb="get" noun="PartyContentLocation"><in-parameters><parameter name="partyId" required="true"/><parameter name="partyContentTypeEnumIdList" type="List" required="true"/><parameter name="getText" type="Boolean" default="false"/></in-parameters><out-parameters><parameter name="contentLocation"/><parameter name="partyContent"/><parameter name="contentText"><description><![CDATA[Only populated if getText is true]]></description></parameter></out-parameters><actions><iterate list="partyContentTypeEnumIdList" entry="partyContentTypeEnumId"><entity-find entity-name="mantle.party.PartyContent" list="partyContentList"><econdition field-name="partyId"/><econdition field-name="partyContentTypeEnumId"/><order-by field-name="-contentDate"/></entity-find><if condition="partyContentList"><set field="partyContent" from="partyContentList[0]"/><break/></if></iterate><set field="contentLocation" from="partyContent ? partyContent.contentLocation : null"/><if condition="getText && partyContent"><if condition="partyContent.contentLocation"><then><set field="contentText" from="ec.resource.getLocationText(partyContent.contentLocation, false)"/></then><else><set field="contentText" from="partyContent.description"/></else></if></if></actions></service>